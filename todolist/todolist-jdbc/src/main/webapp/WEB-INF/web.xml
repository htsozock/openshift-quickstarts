<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">

  <display-name>ProjectCRUDOperations</display-name>
  
    <welcome-file-list>
      <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
  
    <servlet>
        <servlet-name>MainServlet</servlet-name> 
         <servlet-class>org.openshift.quickstarts.todolist.servlet.MainServlet</servlet-class>
     </servlet>
    
    
    
    <servlet>
     <servlet-name>MainServletActivities</servlet-name>
     <servlet-class>org.openshift.quickstarts.todolist.servlet.MainServletActivities</servlet-class>
     </servlet>

     <servlet>
     <servlet-name>MainServletIndicators</servlet-name>
     <servlet-class>org.openshift.quickstarts.todolist.servlet.MainServletIndicators</servlet-class>
     </servlet>

    <servlet-mapping>
     <servlet-name>MainServlet</servlet-name>
        <url-pattern>/MainServlet</url-pattern> 
       
    </servlet-mapping>
    
    <servlet-mapping>
       <servlet-name>MainServletActivities</servlet-name>
        <url-pattern>/MainServletActivities</url-pattern>
      </servlet-mapping>
  <servlet-mapping>
       <servlet-name>MainServletIndicators</servlet-name>
        <url-pattern>/MainServletIndicators</url-pattern>
      </servlet-mapping>

</web-app>


